package cliente;

public class Cliente {
	
	//Criando atributos do cliente: 
	
	private String nome;
	private String genero;
	private float idade;
	

	//Criando metodos do cliente
	public void setCliente (String nome, String genero, float idade){
		
		this.nome = nome;
		this.genero = genero;
		this.idade = idade;
		
	}
	
	public String getNome () {
		return nome;
	}
	
	public String getGenero () {
		return genero;
	}
	
	public void getIdade(int idade) {
		
		this.idade = idade;
		
	}
	
	
	
	/*
	 * */

}
